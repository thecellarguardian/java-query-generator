element Person
{
	key {Genoma personalSequence;}
	key {int identityCode; string nation;}
	{
		string name;
		string surname;
		int age;
		char sex;
	}
}

element Nation
{
	key{string nationName;}
	{PersonList inhabitants;}
}

container<Person> Nation;
container<Nation> Nation;
container<Person> Nation1;

query<Person> echelon(Nation);

